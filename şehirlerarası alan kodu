comboBox1.Items.Add(AlanKodu.ANTALYA);
comboBox1.Items.Add(AlanKodu.İZMİR);
comboBox1.Items.Add(AlanKodu.KONYA); }
private void button1_Click(object sender, EventArgs e) 
{
